<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - My ASP.NET MVC Application</title>
      <link href="~/Images/dotnetsaifavicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <script type="text/javascript">
        function preventBack(){window.history.forward();}
        setTimeout("preventBack()", 0);
        window.onunload=function(){null};
    </script>
</head>
<body>


    <header>

        <div class="col-md-12 col-sm-12">
            <section id="login">
                <div class="panel">

                    <div class="panel-body">
                        <div class="btn btn-warning btn-pressure">
                            @*<a href="/Home/Contact">Team Page</a>*@
                        </div>
                        <div class="btn btn-warning btn-pressure">
                            @*<a href="/Home/About">About Us</a>*@
                        </div>
                    </div>
                </div>

            </section>
        </div>

    </header>
    <div id="body">
        <div class="container">
            @RenderSection("featured", required: false)
            <section class="">
                @RenderBody()
            </section>
        </div>
    </div>
    <footer>
        <div class="container">
            <div class="text-center footer">
                Developed By <a href="180104010@aust.edu">Shimanta Sarker,Shawly Rohman and Jain Jannath</a>
            </div>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
